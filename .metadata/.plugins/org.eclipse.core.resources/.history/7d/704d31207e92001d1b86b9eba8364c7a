package br.com.magna.sistemas.veiculos.modelo;

public class VeiculosMotorizados extends Veiculos {
	
	private double Motor;
	private int potencia;
	private int VeloMaxima;
	
	public double getMotor() {
		return Motor;
	}
	public void setMotor(double motor) {
		Motor = motor;
	}
	public int getPotencai() {
		return potencia;
	}
	public void setPotencai(int potencia) {
		this.potencia = potencia;
	}
	public int getVeloMaxima() {
		return VeloMaxima;
	}
	public void setVeloMaxima(int veloMaxima) {
		VeloMaxima = veloMaxima;
	}
	
	 public void acelerar() {
	        System.out.println("Veiculo acelerando !!!");
	    }
	 
	 public void freiar() {
	        System.out.println("Veiculo freiando !!!");
	    }
	 
	 public String abrirPorta() {
	        System.out.println("O veiculo abriu a porta !!!");
	    }
	 
	 public void fecharPorta() {
	        System.out.println("O veiculo fechar a porta !!!");
	    }
	 
	 public void passarMarcha() {
	        System.out.println("O veiculo passou a marcha !!!");
	    }

}
