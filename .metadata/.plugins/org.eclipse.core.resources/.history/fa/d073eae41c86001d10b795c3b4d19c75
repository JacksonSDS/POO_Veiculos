package br.com.beecrowd;

import java.io.IOException;
import java.util.Scanner;


	public class Main2 {
		   
	public static void main(String[] args) throws IOException {
		        int A, B, PROD;

		        try (Scanner input = new Scanner(System.in)) {
					A = input.nextInt();
					B = input.nextInt();
				}

		        PROD = A * B;
		        
		        System.out.printf("PROD = %d\n", PROD);
		    }
		}

	


