package tarefa_1;

public class VeiculosNaoMotorizados {
	
	private boolean marcha;
	private boolean portas;
	private boolean janelas;
	
	public boolean Marcha() {
		return marcha;
	}
	public void setMarcha(boolean marcha) {
		this.marcha = marcha;
	}
	public boolean Portas() {
		return portas;
	}
	public void setPortas(boolean portas) {
		this.portas = portas;
	}
	public boolean isJanelas() {
		return janelas;
	}
	public void setJanelas(boolean janelas) {
		this.janelas = janelas;
	}

}
